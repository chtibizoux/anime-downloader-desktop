{
  "name": "anime_download-electron",
  "version": "1.0.0",
  "description": "A windows app to download animes",
  "main": "main.js",
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "electron": "^16.2.5",
    "electron-builder": "^22.14.13"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "download",
    "anime"
  ],
  "build": {
    "appId": "fr.chtibizoux.animedownloader",
    "productName": "Anime Donwloader",
    "win": {
      "icon": "./images/icon.jpg"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "author": "Cl√©ment Songis",
  "license": "ISC"
}
